    <Lib>
      <AdditionalLibraryDirectories>$(MSBuildThisFileDirectory)..\$(Platform)\v$(PlatformToolsetVersion)\$(Configuration)\;%(AdditionalLibraryDirectories)</AdditionalLibraryDirectories>
      <AdditionalDependencies>sqlite3++.lib;%(AdditionalDependencies)</AdditionalDependencies>
    </Lib>
  </ItemDefinitionGroup>
  <ItemGroup />
</Project>
